ui_print(">>");
ui_print(">> CyanogenMod Bootimage Updater");
ui_print(">>   for Samsung Galaxy Tab 2");
ui_print(">>");

assert(getprop("ro.product.device") == "p3113" || getprop("ro.build.product") == "p3113" || getprop("ro.product.board") == "p3113" || 
       getprop("ro.product.device") == "GT-P3113" || getprop("ro.build.product") == "GT-P3113" || getprop("ro.product.board") == "GT-P3113" ||
       getprop("ro.product.device") == "espressowifi" || getprop("ro.build.product") == "espressowifi" || getprop("ro.product.board") == "espressowifi");

show_progress(0.200000, 2);
mount("ext4", "EMMC", "/dev/block/mmcblk0p9", "/system");
show_progress(0.500000, 10);
package_extract_dir("system", "/system");
show_progress(0.300000, 0);

assert(package_extract_file("boot.img", "/tmp/boot.img"),
       write_raw_image("/tmp/boot.img", "/dev/block/mmcblk0p5"),
       delete("/tmp/boot.img"));

unmount("/system");
ui_print("Done.");
